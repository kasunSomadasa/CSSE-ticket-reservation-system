<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sliit.csse.ticketscanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Scanner" />
        <activity android:name=".UserHome" />
        <activity android:name=".RideList" />
        <activity android:name=".SingleRide" />
        <activity android:name=".ViewAccount" />
        <activity android:name=".StartRide" />
        <activity android:name=".EndRide"></activity>
    </application>

</manifest>