<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.sliit.csse.ticketscanner.SingleRide">

    <TextView
        android:id="@+id/fromView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="87dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/toView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/fromView"
        android:layout_below="@+id/fromView"
        android:layout_marginTop="36dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/startDateView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/toView"
        android:layout_below="@+id/toView"
        android:layout_marginTop="52dp"
        android:text="TextView" />
</RelativeLayout>
